<!DOCTYPE html>
<html lang="en">
<head>
<meta charset="UTF-8">

<!-- If IE use the latest rendering engine -->
<meta http-equiv="X-UA-Compatible" content="IE=edge">

<!-- Set the page to the width of the device and set the zoon level -->
<meta name="viewport" content="width = device-width, initial-scale = 1">
<title>Hotel Management System - Pricing</title>
<link rel="stylesheet" type="text/css" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap.min.css">
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/js/bootstrap.min.js"></script>

<style>
body, html {
  height: 100%;
}

.bg {
    /* The image used */
    background-image: url("data:image/jpeg;base64,");

    /* Full height */
    height: 100%;
    position: absolute;
    width: 100%;
    left: 0;

    /* Center and scale the image nicely */
    background-position: center;
    background-repeat: no-repeat;
    background-size: cover;
}

.glyphicon.glyphicon-home {
    font-size: 20px;
    color: black;
}

.jumbotron {
    background-color:#2E2D88;
    color:white;
}

.priceInput {
    width: 100%;
    text-align: center;
}
/* Adds borders for tabs */
.tab-content {
    border-left: 1px solid #ddd;
    border-right: 1px solid #ddd;
    border-bottom: 1px solid #ddd;
    padding: 10px;
}

.nav-tabs, .nav-pills {
    text-align:center;
    height: 20%;
    margin-bottom: 0;
}

.nav-tabs > li, .nav-pills > li {
    float:none;
    display:inline-block;
    *display:inline; /* ie7 fix */
    zoom:1; /* hasLayout ie7 trigger */

}

.nav>li>a {
    padding-top: 20px;
    padding-bottom: 20px;
    color:black;
}

#home-navbar {
    position: relative;
    width: 100%;
    left: 0;
}

#login-btn {
    padding-top: 5px;
    margin-top: 10px;
    margin-bottom: 10px;
    display:inline-block;
    *display:inline; /* ie7 fix */
    zoom:1; /* hasLayout ie7 trigger */
}

#header-label {
    padding-top: 10px;
    text-align: center;
    color: black;
    font-size: 3em;
}

#profile-indicator {
    padding-top: 10px;
    text-align: center;
}

#viewrooms-btn {
    padding-top: 20px;
    text-align: center;
}

#viewrooms-btn>a {
    background-color:burlywood;
    border:none;
}
</style>

</head>
<body>
<div class="container bg">

    <!-- Navigation Bar -->
    <div class="container-fluid">
    <div class="row" id="home-navbar">

    <!-- Use nav-pills, nav-tabs. To center tabs add nav-justified to the class for ul -->
    <ul class="nav nav-pills">
        
        <div class="nav nav-pills col-lg-3 col-md-3 col-sm-3 col-xs-3">
            <% if(userData != 'undefined' && userType != 'client') { %>
            <li class="text-center"><a href="../staff/schedule">SCHEDULE</a></li>
            <% } else { %>
            <li class="text-center"><a href="/">HOME</a></li>
            <% } %>
        </div>

        <div class="nav nav-pills col-lg-3 col-md-3 col-sm-3 col-xs-4">
            <li class="text-center"><a href="accommodation">ACCOMMODATION</a></li>
        </div>

        <div class="nav nav-pills col-lg-3 col-md-3 col-sm-3 col-xs-3">
            <li class="active text-center"><a href="pricing">PRICING</a></li>
        </div>

        <% if(userData != 'undefined') { %>
            <div class="nav nav-pills col-lg-2 col-md-2 col-sm-2 col-xs-2" id="profile-indicator">
                <a href="../dashboard"><span class="glyphicon glyphicon-home"></span></a>
                <p>Logged in as <%= userData.user.username %></p>
            </div>
            <div class="nav nav-pills col-lg-1 col-md-1 col-sm-1 col-xs-2">
                <a href="/users/logout"><button type="submit" class="btn btn-danger" role="button" id="login-btn">LOGOUT</button></a>
            </div>
        <% } else { %>
        <div class="nav nav-pills col-lg-3 col-md-3 col-sm-3 col-xs-2">
            <a href="/users/login"><button type="submit" class="btn btn-danger" role="button" id="login-btn">LOGIN</button></a>
        </div>
        <% } %>
    </ul>
        
    </div> <!-- home-navbar -->
    </div> <!-- container-fluid // END OF TOP NAV BAR // -->

    <div class="container-fluid">
        <div class="row">
            <div class="col-lg-8">
                <h2 style="text-align: center; color: black;">Rooms</h2>
                <div id="myCarousel" class="carousel slide" data-ride="carousel">
                    <!-- Indicators -->
                    <ol class="carousel-indicators">
                    <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
                    <li data-target="#myCarousel" data-slide-to="1"></li>
                    <li data-target="#myCarousel" data-slide-to="2"></li>
                    </ol>

                    <!-- Wrapper for slides -->
                    <div class="carousel-inner">
                    <div class="item active">
                        <img src="./images/room.jpg" alt="Los Angeles" style="width:100%; height:500px">
                        <div class="carousel-caption d-none d-md-block">
                            <h5 id="roomtype1"><%= roomsType[0].type %></h5>
                            <div class="row">
                                <div class="col-lg-4"></div>
                                <div class="col-lg-4">
                                    <p>Adult: <%= roomsType[0].price_adult %>&euro;</p>
                                    <% if (userType == 'manager') { %>
                                        <form role="form" action="/rooms/editAdultPrice" method="POST">
                                            <input type="hidden" name="roomType">
                                            <input type="text" class="form-control priceInput" placeholder="New price" name="newAdultPrice"> 
                                            <button type="submit" class="btn btn-primary" onclick="return beforeSubmit1()">Edit</button>    
                                        </form>
                                    <% } %>
                                </div>
                                <div class="col-lg-4"></div>
                            </div>
                            <div class="row">
                                <div class="col-lg-4"></div>
                                <div class="col-lg-4">
                                    <p>Child: <%= roomsType[0].price_child %>&euro;</p>
                                    <% if (userType == 'manager') { %>
                                    <form role="form" action="/rooms/editChildPrice" method="POST">
                                        <input type="hidden" name="roomType">
                                        <input type="text" class="form-control priceInput" placeholder="New price" name="newChildPrice"> 
                                        <button type="submit" class="btn btn-primary" onclick="return beforeSubmit2()">Edit</button>    
                                    </form>
                                    <% } %>
                                </div>
                                <div class="col-lg-4"></div>
                            </div>
                        </div>
                    </div>

                    <div class="item">
                        <img src="./images/room.jpg" alt="Chicago" style="width:100%; height:500px">
                        <div class="carousel-caption d-none d-md-block">
                            <h5 id="roomtype2"><%= roomsType[1].type %></h5>
                            <div class="row">
                                <div class="col-lg-4"></div>
                                <div class="col-lg-4">
                                    <p>Adult: <%= roomsType[1].price_adult %>&euro;</p>
                                    <% if (userType == 'manager') { %>
                                        <form role="form" action="/rooms/editAdultPrice" method="POST">
                                            <input type="hidden" name="roomType">
                                            <input type="text" class="form-control priceInput" placeholder="New price" name="newAdultPrice"> 
                                            <button type="submit" class="btn btn-primary" onclick="return beforeSubmit3()">Edit</button>    
                                        </form>
                                    <% } %>
                                </div>
                                <div class="col-lg-4"></div>
                            </div>
                            <div class="row">
                                <div class="col-lg-4"></div>
                                <div class="col-lg-4">
                                    <p>Child: <%= roomsType[1].price_child %>&euro;</p>
                                    <% if (userType == 'manager') { %>
                                    <form role="form" action="/rooms/editChildPrice" method="POST">
                                        <input type="hidden" name="roomType">
                                        <input type="text" class="form-control priceInput" placeholder="New price" name="newChildPrice"> 
                                        <button type="submit" class="btn btn-primary" onclick="return beforeSubmit4()">Edit</button>    
                                    </form>
                                    <% } %>
                                </div>
                                <div class="col-lg-4"></div>
                            </div>
                        </div>
                    </div>
                    
                    <div class="item">
                        <img src="./images/room.jpg" alt="New york" style="width:100%; height:500px">
                        <div class="carousel-caption d-none d-md-block">
                            <h5 id="roomtype3"><%= roomsType[2].type %></h5>
                            <div class="row">
                                <div class="col-lg-4"></div>
                                <div class="col-lg-4">
                                    <p>Adult: <%= roomsType[2].price_adult %>&euro;</p>
                                    <% if (userType == 'manager') { %>
                                        <form role="form" action="/rooms/editAdultPrice" method="POST">
                                            <input type="hidden" name="roomType">
                                            <input type="text" class="form-control priceInput" placeholder="New price" name="newAdultPrice"> 
                                            <button type="submit" class="btn btn-primary" onclick="return beforeSubmit5()">Edit</button>    
                                        </form>
                                    <% } %>
                                </div>
                                <div class="col-lg-4"></div>
                            </div>
                            <div class="row">
                                <div class="col-lg-4"></div>
                                <div class="col-lg-4">
                                    <p>Child: <%= roomsType[2].price_child %>&euro;</p>
                                    <% if (userType == 'manager') { %>
                                    <form role="form" action="/rooms/editChildPrice" method="POST">
                                        <input type="hidden" name="roomType">
                                        <input type="text" class="form-control priceInput" placeholder="New price" name="newChildPrice"> 
                                        <button type="submit" class="btn btn-primary" onclick="return beforeSubmit6()">Edit</button>    
                                    </form>
                                    <% } %>
                                </div>
                                <div class="col-lg-4"></div>
                            </div>
                        </div>
                    </div>
                    </div>

                    <!-- Left and right controls -->
                    <a class="left carousel-control" href="#myCarousel" data-slide="prev">
                    <span class="glyphicon glyphicon-chevron-left"></span>
                    <span class="sr-only">Previous</span>
                    </a>
                    <a class="right carousel-control" href="#myCarousel" data-slide="next">
                    <span class="glyphicon glyphicon-chevron-right"></span>
                    <span class="sr-only">Next</span>
                    </a>
                </div>
            </div>
            <div class="col-lg-4">
            </div>
        </div>
        <% include ./partials/messages %>
    </div>

</div>

<!-- Set the data for the hidden inputs -->
<script>
    function beforeSubmit1() {
        document.getElementsByName('roomType')[0].value = document.getElementById('roomtype1').innerText
    }
</script>

<script>
    function beforeSubmit2() {
        document.getElementsByName('roomType')[1].value = document.getElementById('roomtype1').innerText
    }
</script>

<script>
    function beforeSubmit3() {
        document.getElementsByName('roomType')[2].value = document.getElementById('roomtype2').innerText
    }
</script>

<script>
    function beforeSubmit4() {
        document.getElementsByName('roomType')[3].value = document.getElementById('roomtype2').innerText
    }
</script>

<script>
    function beforeSubmit5() {
        document.getElementsByName('roomType')[4].value = document.getElementById('roomtype3').innerText
    }
</script>

<script>
    function beforeSubmit6() {
        document.getElementsByName('roomType')[5].value = document.getElementById('roomtype3').innerText
    }
</script>

</body>
</html>
